void loop() {

  //Check whether Serial communication is available
  if (Serial.available()) {

    char tune = Serial.read();
    
    //If it recieves an F, play Fur Elise.  
    if(tune == 'F' | tune == 'f') {

       midi_furelise();
    }
    if(tune == 'J' | tune == 'j') {

       midi_jingle();
    }
    if(tune == 'E' | tune == 'e') {

       midi_eden();
    }
    // Error message
    else
    {
      Serial.println("Unknown command. Press F or J");
    }
    
  }
    
